# Environment variables for Fintrace Backend
# Copy this file to .env for local development

# Server Configuration
PORT=8000

# CORS Configuration (comma-separated origins)
# For local development, use *
# For production, set your Vercel frontend URL
ALLOWED_ORIGINS=*

# Production example (update after deploying frontend):
# ALLOWED_ORIGINS=https://your-app.vercel.app,https://your-app-*.vercel.app

# Optional: Override detection thresholds
# SMURFING_MIN_ENDPOINTS=10
# SMURFING_WINDOW_HOURS=72
# VELOCITY_WINDOW_HOURS=24
